$ (bubble-sort) [<list.l>] defunc
    len [] swap {1} - {<count.l>} swap

    [<i.l> -1 <count.l> 1] for
        [<a.l>] [<list.l> <i.l>] at merge
    
        [<j.l> <i.l>] [<count.l> 1] - {1} merge for
            [<list.l> <j.l>] at {<b.l>} swap

            [<a.l>] get get {<a-val.l>} swap
            [<b.l>] get get {<b-val.l>} swap

            [<a-val.l> <b-val.l>] > if
                [<a.l>] get [<b-val.l>] get =
                [<b.l>] get [<a-val.l>] get =
            end
        end
    end

    [<list.l>] at get-list return
end

